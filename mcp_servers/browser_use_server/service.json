        "query": {
          "type": "string",
          "description": "搜索查询词",
          "required": true
        }
      },
      "examples": [
        {
          "query": "Python machine learning tutorial"
        },
        {
          "query": "browser automation tools"
        }
      ]
    },
    {
      "name": "browser_go_back",
      "description": "返回上一页",
      "parameters": {},
      "examples": [
        {}
